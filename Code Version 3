Idea = input("1 or 2 MCI, 1a MMi, 2a MMb 3CM, 4C, 5CP, 6D, 0I , nn to Genre, x for more info ")
if Idea=="1":
	print('Main melody:', random.choice(MM[0]))
	print('Counter melody:', random.choice(CM))
	print('Idea:', random.choice(I))
	print('Idea:', random.choice(I))
	
	
   

	  
elif Idea=="2":
	print('Main Melody:', random.choice(MM[1]))
	print('Counter Melody:', random.choice(CM))
	print('Idea:', random.choice(I))
	print('Idea:', random.choice(I))
	
elif Idea=="1a":
	print('Main Melody:', random.choice(MM[0]))

elif Idea=="2a":
	print('Main Melody:', random.choice(MM[1]))

elif Idea=="3":
	print('Counter Melody:', random.choice(CM))
		
elif Idea=="4":
	print('Chord Progression:', random.choice(C))			

elif Idea=="5":
	print('Chord Pattern:', random.choice(CP))
	
elif Idea=="6":
	print('Drums and Bass:', random.choice(D[35]))

elif Idea=="0":
	print('Idea:', random.choice(I))				
		
elif Idea=="s":
	print('Sound Selection:', random.choice(SS))	

